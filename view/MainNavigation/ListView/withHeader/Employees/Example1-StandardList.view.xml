<mvc:View height="100%" 
    controllerName="UI5FioriForTools.controller.MainNavigation.ListView.Employees" 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m">

    <List headerText="Items ({=${/results}.length})" 
        items="{path:'/results', templateShareable: true}">
        <items>
            <StandardListItem icon="sap-icon://person-placeholder" 
                iconInset="false" 
                type="Navigation" 
                press="onPress"
                title="{LastName}, {FirstName}" 
                description="{Role}" 
                info="{ProjectStatus}" 
                infoState="{ProjectStatusInfoState}"
                highlight="{ProjectStatusInfoState}">
            </StandardListItem>
        </items>
    </List>
    
</mvc:View>
 